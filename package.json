{
  "name": "ets",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "build": "tsc -p tsconfig-commonjs.json && tsc -p tsconfig-module.json",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "electron ./web/main.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start-typescript.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^17.1.2",
    "typescript": "^4.5.5"
  },
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start-typescript/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start-typescript#readme",
  "main": "./win/index.js",
  "dependencies": {
    "asar": "^3.1.0",
    "serialport": "^10.3.0"
  }
}
